{
  "manifest_version": 2,
  "name": "View Images",
  "version": "1.0",
  "description": "Restore the View Image button on Google Images",
  "author": "weiwei",
  "permissions": [
    "https://*.google.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.google.com/*"],
      "js": ["jquery-3.3.1.min.js", "script.js"]
    }
  ]
}